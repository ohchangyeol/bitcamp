package ex1;

public class Prob {

	public static void main(String[] args) {
		int inputInt = Integer.parseInt(args[0]);
			
		
		for (int i = 0 ; i < inputInt ; i++){
			String sum = "";
			for(int j = i; j < inputInt; j++ ) {
				sum = "*";
				System.out.println(" j 실행");			
			}
			System.out.println(sum);
		}
		
		
		
		
		
		
		
